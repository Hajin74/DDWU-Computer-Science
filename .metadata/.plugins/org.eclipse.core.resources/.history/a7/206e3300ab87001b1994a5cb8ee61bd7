public class Practice23 {
	
	public static void intArray(int[] arr) {
		
	}
	
	public static void main(String[] args) {
		int[][] arr1 = new int[3][4];
		
		for(int i = 0; i < 3; i++) {
			for(int j = 0; j < 4; j++) {
				arr1[i][j] = (j + 1) * (i + 1) * 10;
			}
		}
		
		

	}

}
